<h1>ユーザ編集</h1>

<%= form_with model: @user do |f| %>

 <p>ユーザ名</p>
 <%= f.text_field :name, autofocus: true, placeholder:"名前" %>

 <p>プロフィール画像</p>
 <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

 <%= f.submit "変更を保存" %>
 <%= link_to "退会する", users_confirm_path(@user) %>
<% end %>